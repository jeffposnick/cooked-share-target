{
  "background_color": "#2196f3",
  "display": "browser",
  "description": "A share target that will redirect to the Cooked.wiki version of the current recipe page.",
  "icons": [
    {
      "sizes": "512x512",
      "src": "icon-512-maskable.png",
      "type": "image/png"
    }
  ],
  "name": "Share to Cooked.wiki",
  "scope": "/",
  "share_target": {
    "action": "./share-target/",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "text": "recipeUrl"
    }
  },
  "short_name": "Cooked.wiki Share",
  "start_url": "/",
  "theme_color": "#2196f3"
}